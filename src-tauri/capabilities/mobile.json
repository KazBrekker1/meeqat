{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Capabilities for mobile platforms",
  "platforms": ["android", "iOS"],
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:window:allow-hide",
    "core:window:allow-show",
    "notification:default",
    "store:default"
  ]
}
